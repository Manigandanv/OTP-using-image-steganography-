					<h2 class="section-title">
					</h2><!-- /.Section-title  -->
					<p class="section-description">
					</p><!-- /.section-description -->

					<form action="WebForm2.aspx" id="contact-form" method="post" class="clearfix">
						<div class="contact-box-hide">
							<div class="col-sm-6">
								<input type="text"  class="form-control" id="uname" name="uname" required placeholder="Username">
								<span class="first-name-error"></span>
							</div>
							<div class="col-sm-6">
								<input type="text"  class="form-control" id="pwd" name="pwd" required placeholder="Password">
								<span class="last-name-error"></span>
							</div>
							<div class="col-sm-6">
								<input type="email" class="form-control"  id="email" name="email" required placeholder="Email Address">
								<span class="contact-email-error"></span>
							</div>
							<div class="col-sm-6">
								<input type="text"  class="form-control" id="phone" name="phone" required placeholder="Phone number">
								<span class="contact-subject-error"></span>
							</div>
							<div class="col-sm-10">
								<textarea class="form-control" rows="5" id="address" name="address" required placeholder="Address"></textarea>
								<span class="contact-message-error"></span>
							</div>
							<div class="col-sm-2">
								<button OnServerclick="insert" id="contact-submit" class="btn custom-btn col-xs-12" type="submit" name="submit"><i class="fa fa-rocket"></i></button>
								<span id="contact-loading" class="btn custom-btn col-xs-12"> <i class="fa fa-refresh fa-spin"></i> </span>
							</div>
						</div><!-- /.contact-box-hide -->
						<div class="contact-message"></div>

					</form><!-- /#contact-form -->		

